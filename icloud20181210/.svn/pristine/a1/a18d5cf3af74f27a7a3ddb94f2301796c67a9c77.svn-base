    padding-left: 5px;
    padding-bottom: 1px;
    border-left: rgb(120,172,255) 1px solid;
    color: black;
    padding-top: 1px;
    border-bottom: rgb(120,172,255) 1px solid; ;HEIGHT:expression(/MSIE 5.0/.test(navigator.userAgent) ? "17px" : "auto");TEXT-DECORATION:none
